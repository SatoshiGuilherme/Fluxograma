#9
print("Bem vindo a calculadora de teste, essa calculadora suporta apenas 2 digitos por equação")
print("capaz de fazer, adição, subtração, multiplicação e divisão")

uno = int(input("digite o primeiro numero:"))
dos = int(input("digite o segundo numero:"))

result_som = uno + dos
result_sub = uno - dos
result_div = uno / dos
result_mult = uno * dos

print("a soma = ", result_som)
print("a subtração = ", result_sub)
print("a divisão = ", result_div)
print("a multiplicação = ", result_mult)

